				"support.constant",
				"variable.language",
				"constant.character.escape",
				"markup.heading"
			],
			"settings": {
				"foreground": "#073642",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#586e75",
				"fontStyle": ""
			}
