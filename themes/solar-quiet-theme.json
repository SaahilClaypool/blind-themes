    "tokenColors": [
        {
            "name": "",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#6A737D"
            }
        },
        {
            "name": "",
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language",
                "support",
                "meta.property-name",
                "string variable",
                "support.constant",
                "meta.object-literal",
                "support.variable",
                "meta.module-reference",
                "markup.raw",
                "markup.ignored",
                "markup.untracked",
                "meta.diff.header",
                "meta.output"
            ],
            "settings": {
                "foreground": "#005CC5"
            }
        },
        {
            "name": "",
            "scope": [
                "keyword.operator.symbole",
                "keyword.other.mark",
                "variable.parameter.function",
                "storage.modifier.package",
                "storage.modifier.import",
                "storage.type.java",
                "variable.other",
                "string source",
                "markup.list",
                "meta.function-call support.function",
                "meta.function-call entity.name.function",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "",
            "scope": [
                "entity",
                "entity.name"
            ],
            "settings": {
                "foreground": "#6F42C1"
            }
        },
        {
            "name": "",
            "scope": [
                "entity.name.tag",
                "markup.quote",
                "markup.inserted",
                "meta.diff.header.to-file",
                "punctuation.definition.inserted"
            ],
            "settings": {
                "foreground": "#22863A"
            }
        },
        {
            "name": "",
            "scope": [
                "keyword",
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#992631"
            }
        },
        {
            "name": "",
            "scope": [
                "string",
                "punctuation.definition.string",
                "string punctuation.section.embedded source",
                "source.regexp",
                "string.regexp",
                "string.regexp.character-class",
                "string.regexp constant.character.escape",
                "string.regexp source.ruby.embedded",
                "string.regexp string.regexp.arbitrary-repitition"
            ],
            "settings": {
                "foreground": "#032F62"
            }
        },
        {
            "name": "",
            "scope": [
                "string.unquoted.import.ada"
            ],
            "settings": {}
        },
        {
            "name": "",
            "scope": [
                "variable",
                "markup.changed",
                "punctuation.definition.changed",
                "sublimelinter.mark.warning"
            ],
            "settings": {
                "foreground": "#E36209"
            }
        },
        {
            "name": "",
            "scope": [
                "invalid.broken",
                "invalid.deprecated",
                "invalid.unimplemented"
            ],
            "settings": {
                "foreground": "#B31D28",
                "fontStyle": "bold italic underline"
            }
        },
        {
            "name": "",
            "scope": [
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#B31D28",
                "fontStyle": "italic underline"
            }
        },
        {
            "name": "",
            "scope": [
                "carriage-return"
            ],
            "settings": {
                "foreground": "#992631",
                "fontStyle": "italic underline"
            }
        },
        {
            "name": "",
            "scope": [
                "message.error",
                "markup.deleted",
                "meta.diff.header.from-file",
                "punctuation.definition.deleted",
                "brackethighlighter.unmatched",
                "sublimelinter.mark.error"
            ],
            "settings": {
                "foreground": "#B31D28"
            }
        },
        {
            "name": "",
            "scope": [
                "string.regexp constant.character.escape"
            ],
            "settings": {
                "foreground": "#22863A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": [
                "markup.heading",
                "markup.heading entity.name",
                "meta.separator"
            ],
            "settings": {
                "foreground": "#005CC5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic"
            }
        },
        {
            "name": "",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": [
                "meta.diff.range"
            ],
            "settings": {
                "foreground": "#6F42C1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": [
                "brackethighlighter.tag",
                "brackethighlighter.curly",
                "brackethighlighter.round",
                "brackethighlighter.square",
                "brackethighlighter.angle",
                "brackethighlighter.quote"
            ],
            "settings": {
                "foreground": "#586069"
            }
        },
        {
            "name": "",
            "scope": [
                "sublimelinter.gutter-mark"
            ],
            "settings": {
                "foreground": "#959DA5"
            }
        },
        {
            "name": "",
            "scope": [
                "constant.other.reference.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#032F62",
                "fontStyle": "underline"
            }
        },
        {
            "name": "",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "",
            "scope": [
                "token.info-token"
            ],
            "settings": {
                "foreground": "#316BCD"
            }
        },
        {
            "name": "",
            "scope": [
                "token.warn-token"
            ],
            "settings": {
                "foreground": "#CD9731"
            }
        },
        {
            "name": "",
            "scope": [
                "token.error-token"
            ],
            "settings": {
                "foreground": "#CD3131"
            }
        },
        {
            "name": "",
            "scope": [
                "token.debug-token"
            ],
            "settings": {
                "foreground": "#800080"
            }
        }
    ]
}